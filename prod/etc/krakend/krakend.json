{
  "$schema": "https://www.krakend.io/schema/krakend.json",
  "version": 3,
  "name": "Twit2 API gateway",
  "timeout": "3000ms",
  "cache_ttl": "300s",
  "output_encoding": "json",
  "port": 8090,
  "endpoints": [
    {
      "endpoint": "/auth/login",
      "method": "POST",
      "output_encoding": "json",
      "backend": [
        {
          "url_pattern": "/login",
          "encoding": "json",
          "sd": "static",
          "method": "POST",
          "host": [
            "http://10.5.3.57:3200"
          ],
          "disable_host_sanitize": false
        }
      ]
    },
	{
      "endpoint": "/auth/register",
      "method": "POST",
      "output_encoding": "json",
      "backend": [
        {
          "url_pattern": "/register",
          "encoding": "json",
          "sd": "static",
          "method": "POST",
          "host": [
            "http://10.5.3.57:3200"
          ],
          "disable_host_sanitize": false
        }
      ]
    }
  ]
}